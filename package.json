{
  "name": "workbox-offline-demo",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf build/ && rimraf docs/ && tsc && rollup -c",
    "build:watch": "tsc --watch",
    "build:dev": "tsc",
    "start": "concurrently \"tsc --watch\" \"web-dev-server --app-index index.html --compatibility none --node-resolve --watch --open\"",
    "dev": "concurrently \"tsc --watch\" \"web-dev-server --app-index index.html --compatibility none --node-resolve --watch --open\"",
    "advanced": "concurrently \"tsc --watch\" \"web-dev-server --app-index advanced.html --compatibility none --node-resolve --watch --open\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-list": "^0.20.0",
    "@material/mwc-menu": "^0.20.0",
    "@pwabuilder/pwainstall": "^1.6.6",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@surma/rollup-plugin-off-main-thread": "^1.4.2",
    "@vaadin/router": "^1.7.2",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "lit-element": "^2.4.0",
    "rollup-plugin-workbox-inject": "^2.0.0"
  },
  "devDependencies": {
    "@open-wc/rollup-plugin-html": "^1.2.3",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-strip": "^2.0.0",
    "@web/dev-server": "^0.1.3",
    "concurrently": "^5.3.0",
    "lit-analyzer": "^1.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^5.2.1",
    "typescript": "^4.0.2",
    "workbox-build": "^6.0.2"
  }
}
